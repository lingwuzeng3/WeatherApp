  // æœç´¢åŸå¸‚
  static searchCity(keyword: string): CityInfo[] {
    if (!keyword || keyword === '') {
      return CityList;
    }
    const result: CityInfo[] = [];
    for (let i = 0; i < CityList.length; i++) {
      const city = CityList[i];
      if (city.name.includes(keyword) || city.province.includes(keyword)) {
        result.push(city);
      }
    }
    return result;
  }

